{
  "builds": [
    {"src": "/FlashCard/main.py", "use": "@now/python"}
  ],
  "routes": [
		{ "src": "/(.*)", "dest": "main.py" },
		{ "src": "/start/{pg}", "dest": "main.py" },
		{ "src": "/next", "dest": "main.py"},
		{ "src": "/prev", "dest": "main.py" },
		{ "src": "/delete", "dest": "main.py" },
		{ "src": "/randomize", "dest": "main.py" }
	]
}
